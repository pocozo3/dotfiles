################################################################################
#
# zshrc.linux Linux(Debian)環境設定
#     ~/.zsh.d/zshrc.linux
#     対話的起動時のみの設定
#
################################################################################

# ================================================
#  tty 時の設定
# ================================================
case $TERM in
    linux*) # tty 時
        fbterm
        ;;
    *) # その他の仮想端末
        ;;
esac
